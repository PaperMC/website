# Configure edge caching for pages using SSR

/downloads/paper
  CDN-Cache-Control: public
  max-age=600
  Cache-Control: public
  max-age=600

/downloads/folia
  CDN-Cache-Control: public
  max-age=600
  Cache-Control: public
  max-age=600

/downloads/waterfall
  CDN-Cache-Control: public
  max-age=600
  Cache-Control: public
  max-age=600

/downloads/velocity
  CDN-Cache-Control: public
  max-age=600
  Cache-Control: public
  max-age=600
