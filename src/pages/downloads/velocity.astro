---
export const prerender = false;
import Layout from "@/layouts/Layout.astro";
import SoftwareDownloadPage from "@/components/data/SoftwareDownloadPage.svelte";
import { fetchDownloadsPageData } from "@/utils/download";

Astro.response.headers.set("Cache-Control", "public, max-age=300, s-maxage=600, stale-while-revalidate=120");
Astro.response.headers.set("CDN-Cache-Control", "public, max-age=600, stale-while-revalidate=120");
const data = await fetchDownloadsPageData("velocity", Astro.locals.runtime.env.WEBSITE_CACHE);
---

<Layout
  title="Velocity Downloads"
  description="Download Velocity, our high-performance Minecraft proxy."
  keywords={["papermc", "minecraft", "performance", "velocity", "downloads", "jar"]}
  canonical="/downloads/velocity"
>
  <SoftwareDownloadPage id="velocity" description="Download Velocity, our high-performance Minecraft proxy." client:load="svelte" {data} />
</Layout>
